<UserControl x:Class="DownloadZingModernUI.Pages.Trợ_giúp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="370" d:DesignWidth="600">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/HowToUse.xaml" Margin="0,-20,0,0">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Cách sử dụng" Source="/Pages/HowToUse.xaml" />
                <mui:Link DisplayName="Về tác giả" Source="/Pages/Settings/About.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
